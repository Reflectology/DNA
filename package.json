{
    "name": "reflectology-visualizer",
    "displayName": "Reflectology Code Visualizer",
    "description": "Generates intricate diagrams from codebases using reflectology principles",
    "version": "0.1.0",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Visualization",
        "Other"
    ],
    "activationEvents": [
        "onCommand:reflectologyVisualizer.generateDiagram"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "reflectologyVisualizer.visualizeAnyCode",
                "title": "Visualize Any Code Structure Using Reflectology",
                "category": "Reflectology"
            },
            {
                "command": "reflectologyVisualizer.generateDiagram",
                "title": "Generate Reflectology Diagram",
                "category": "Reflectology"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "reflectologyVisualizer.visualizeAnyCode",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npx tsc -p ./",
        "watch": "npx tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^14.0.0",
        "@types/vscode": "^1.60.0",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "d3": "^7.6.1"
    }
}
