{
    "name": "m0wer-visualizer",
    "displayName": "m0wer Code Visualizer",
    "description": "Generates intricate diagrams from codebases using M0WER principles",
    "version": "0.1.1",
    "publisher": "M0WER.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/Reflectology/M0WER"},
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Visualization",
        "Other"
    ],
    "activationEvents": [
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "mower.visualizeAnyCode",
                "title": "Visualize Any Code Structure Using m0wer",
                "category": "m0wer",
                "icon": "$(eye)"
            },
            {
                "command": "mower.generateDiagram",
                "title": "Generate M0WER Diagram",
                "category": "m0wer",
                "icon": "$(graph)"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "mower.visualizeAnyCode",
                    "group": "navigation"
                }
            ],
            "view/title": [
                {
                    "command": "mower.visualizeAnyCode",
                    "when": "view == m0werEntities",
                    "group": "navigation"
                },
                {
                    "command": "mower.generateDiagram",
                    "when": "view == m0werEntities",
                    "group": "navigation@2"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "m0wer",
                    "title": "m0wer",
                    "icon": "resources/mower.svg"
                }
            ],
            "panel": [
                {
                    "id": "m0werPanel",
                    "title": "m0wer",
                    "icon": "resources/mower.svg"
                }
            ]
        },
        "views": {
            "m0wer": [
                {
                    "id": "m0werEntities",
                    "name": "Entities",
                    "icon": "resources/mower.svg"
                }
            ]
                   }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "npx tsc -p ./",
        "watch": "npx tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.60.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
    }
}
